      <Button layoutX="59.0" layoutY="187.0" mnemonicParsing="false" onAction="#savedetails" prefHeight="26.0" prefWidth="697.0" text="Save details" />
      <Label fx:id="statusDONEorUNDONE" layoutX="59.0" layoutY="268.0" prefHeight="66.0" prefWidth="274.0" text="Status PDF: " />
