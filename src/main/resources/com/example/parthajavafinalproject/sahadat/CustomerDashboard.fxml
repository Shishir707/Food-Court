<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="624.0" prefWidth="680.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.parthajavafinalproject.sahadat.CustomerDashboardController">
   <children>
      <Label fx:id="label" layoutX="111.0" layoutY="14.0" prefHeight="35.0" prefWidth="348.0" text="     Customer Dashboard" textFill="#d31010">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Button layoutX="29.0" layoutY="65.0" mnemonicParsing="false" onAction="#CustomerOrderFood" prefHeight="36.0" prefWidth="153.0" text="Place Order">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="29.0" layoutY="233.0" mnemonicParsing="false" onAction="#CustomerTrackOrder" prefHeight="30.0" prefWidth="153.0" text="Track Order">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="27.0" layoutY="397.0" mnemonicParsing="false" onAction="#CustomerShareOrderView" prefHeight="30.0" prefWidth="153.0" text="Share Order">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="29.0" layoutY="454.0" mnemonicParsing="false" onAction="#CustomerLoyaltyPoints" prefHeight="30.0" prefWidth="153.0" text="Earn &amp; Redeem points" />
      <Button layoutX="273.0" layoutY="504.0" mnemonicParsing="false" onAction="#backButton" prefHeight="36.0" prefWidth="134.0" text="Back to login">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="29.0" layoutY="179.0" mnemonicParsing="false" onAction="#CustomerOrderCancel" prefHeight="30.0" prefWidth="153.0" text="Cancel an Order">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="29.0" layoutY="126.0" mnemonicParsing="false" onAction="#CustomerFeedbackView" prefHeight="30.0" prefWidth="153.0" text="Feedback">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button layoutX="30.0" layoutY="288.0" mnemonicParsing="false" onAction="#CustomerRefundRequest" prefHeight="30.0" prefWidth="153.0" text=" Request a Refund" />
      <Button layoutX="27.0" layoutY="340.0" mnemonicParsing="false" onAction="#CustomerFoodAddFavorite" prefHeight="30.0" prefWidth="153.0" text="Add Food to Favorites" />
   </children>
</AnchorPane>
